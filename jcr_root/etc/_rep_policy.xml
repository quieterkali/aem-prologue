<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="sling-tenant"
        rep:privileges="{Name}[jcr:write]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="sling-mapping"
        rep:privileges="{Name}[jcr:read]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="sling-readall"
        rep:privileges="{Name}[jcr:read]"/>
    <deny3
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:all]"/>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="clientlibs-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="contributor"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob=""/>
    </allow5>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="contributor"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/jcr:primaryType"/>
    </allow6>
    <allow7
        jcr:primaryType="rep:GrantACE"
        rep:principalName="reference-adjustment-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow8
        jcr:primaryType="rep:GrantACE"
        rep:principalName="reference-adjustment-service"
        rep:privileges="{Name}[rep:alterProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*/jcr:content"/>
    </allow8>
    <allow9
        jcr:primaryType="rep:GrantACE"
        rep:principalName="reference-adjustment-service"
        rep:privileges="{Name}[rep:alterProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*/jcr:content/*"/>
    </allow9>
    <allow10
        jcr:primaryType="rep:GrantACE"
        rep:principalName="version-manager-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,rep:write]"/>
    <allow11
        jcr:primaryType="rep:GrantACE"
        rep:principalName="design-cache-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow12
        jcr:primaryType="rep:GrantACE"
        rep:principalName="polling-importer-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow13
        jcr:primaryType="rep:GrantACE"
        rep:principalName="wcm-workflow-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow14
        jcr:primaryType="rep:GrantACE"
        rep:principalName="offloading-service"
        rep:privileges="{Name}[jcr:read,crx:replicate,rep:write]"/>
    <allow15
        jcr:primaryType="rep:GrantACE"
        rep:principalName="compat-codeupgrade-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:modifyAccessControl,jcr:read,jcr:readAccessControl,rep:write]"/>
    <allow16
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dynamicmedia-config-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow17
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dam-replication-service"
        rep:privileges="{Name}[jcr:read,rep:write]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/replication/agents*"/>
    </allow17>
    <allow18
        jcr:primaryType="rep:GrantACE"
        rep:principalName="repository-change-listener-service"
        rep:privileges="{Name}[jcr:read]"/>
</jcr:root>
